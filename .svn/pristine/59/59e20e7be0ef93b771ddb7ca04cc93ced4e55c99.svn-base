 /*导航*/
 
 .goods_nav {
 	width: 100%;
 	background-color: #fff;
 	border-radius: 0 0 3.7vw 3.7vw;
 }
 
 .goods_nav_kuan {
 	width: 83%;
 	font-size: 3.7vw;
 	color: #333;
 	display: flex;
 	align-items: center;
 	justify-content: space-around;
 	margin: 0 auto;
 	padding-top: 3.5vw;
 	padding-bottom: 3.7vw;
 }
 
 .nav_title {
 	width: 19vw;
 	text-align: center;
 }
 
 .f_active {
 	font-weight: bold;
 }
 
