html,
body {
	min-height: 100%;
	font-size: 0.9em;
}

body {
	margin: 0px;
	padding: 0px;
	font-family: "Microsoft YaHei", "simsun", "Helvetica Neue", Arial, Helvetica, sans-serif;
	font-size: 0.9em;
	color: #333;
	background: #fff;
}

a:hover {
	text-decoration: none;
}

a {
	color: #666;
	text-decoration: none;
}

img {
	border: 0
}

ul {
	padding: 0;
	margin: 0;
}

ul,
li {
	list-style: none
}

ul,
li,
div {
	box-sizing: border-box
}


/*清理浮动的方法*/

.clearfix:after {
	content: " ";
	display: block;
	clear: both;
	height: 0;
}

.clearfix {
	zoom: 1;
}


/* CSS Document */

.header_swiper {
	width: 100%;
	position: relative;
}

.header_search {
	width: 100%;
	height: 11vw;
	position: absolute;
	top: 0;
	left: 0;
	z-index: 50;
}

.header_search span.position {
	width: 3.5vw;
	height: 5vw;
	margin: 2.5vw 0 0 3vw;
	float: left;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 100% 100%;
}

.header_search p.city {
	height: 11vw;
	display: table;
	float: left;
	line-height: 11vw;
	color: white;
	font-size: 3.5vw;
	padding: 0 2vw
}

.header_search div.search_div {
	position: absolute;
	top: 2vw;
	left: 25%;
	width: 75%;
	height: 7vw;
	
}

.header_search div.search_div input {
	width: 71%;
    height: 9vw;
    padding: 0 2vw 0 4vw;
    box-sizing: border-box;
    background: #fff;
    display: flex;
    align-items: center;
    border-radius: 5vw;
    margin-left: 0vw;
    padding-top: 1vw;
    border:1px solid #fff;
    font-size: ;
}

.header_search div.search_div span.search {
	width: 5vw;
	height: 4.5vw;
	position: absolute;
	top: 1vw;
	left: 1vw;
	z-index: 999;
	background-repeat: no-repeat;
	background-position: center center;
	background-size: 100% 100%;
	margin-left: 2vw;
	margin-right: 2vw;
}

input::-webkit-input-placeholder {
	/* WebKit browsers */
	color: white;
	font-weight: 300;
}

:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: white;
	font-weight: 300;
}

::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: white;
	font-weight: 300;
}

:-ms-input-placeholder {
	/* Internet Explorer 10+ */
	color: white;
	font-weight: 300;
}

.nearby_nav {
	border-bottom: 2vw solid #efeff4;
	border-top: 2vw solid #efeff4;
	overflow: hidden;
	padding: 4vw 0;
}

.nav_ul li img.nav_img {
	width: 12vw;
	height: 12vw
}

.nav_ul li p {
	line-height: 8vw;
	font-size: 3.5vw
}

