/**************************************************商品列表********************************************************************/

.good_list {
	padding: 2.5vw;
}

.good_list ul {
	padding: 0;
}

.good_list ul li {
	padding: 2vw 0;
	border-bottom: 1px solid #eee;
}

.good_list ul li .good_img {
	width: 22vw;
	height: 22vw;
	background: #f2f2f2;
	float: left;
}

.good_list ul li .good_right {
	float: right;
	width: calc(100% - 22vw - 3vw);
}

.good_list ul li .good_right h6 {
	font-size: 4vw;
	color: #000;
}

.good_list ul li .good_right .good_sp {
	font-size: 3vw;
	color: #999999;
}

.good_list ul li .good_right p {
	margin-bottom: 1vw;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.good_list ul li .good_right p span {
	font-size: 3.5vw;
	color: #555555;
}