.section_near_product {
	background: #fff;
}

.section_near_product .ul_near_product {
	width: 100%;
}

.section_near_product .ul_near_product li {
	width: 95%;
	margin: 2vw auto;
	padding: 2vw 0;
	background: white;
	border-top: 2px solid #eee;
	position: relative
}

.section_near_product .ul_near_product li img.product {
	width: 25vw;
	height: 25vw;
	float: left;
	border-radius:3vw ;
}

.section_near_product .ul_near_product li .div_product_info {
	width: 55%;
	float: left;
	margin-left: 3vw
}

.div_product_info h2.good_name {
	font-size: 3.7vw;
	color: #140704;
	line-height: 6vw;
	font-weight: 600;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.div_product_info h2.good_name font {
	background: #ea2244;
	color: white;
	border-radius: 5px;
	font-size: 12px;
	font-weight: 300;
	padding: 0 3px;
}

.div_product_info h3.address {
	font-size: 3.5vw;
	color: #666;
	background-repeat: no-repeat;
	background-position: 0 center;
	background-size: 3.5vw 4vw;
	padding-left: 4vw;
	line-height: 8vw;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.div_product_info p.recomment {
	color: #b7b7b7;
	font-size: 3vw;
	line-height: 7vw;
	width: 100%;
	float: left;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
}

.section_near_product .ul_near_product li p.distance {
	width: 14%;
	text-align: right;
	font-size: 3vw;
	color: #6A6A6A;
	line-height: 6vw;
	overflow: hidden;
	white-space: nowrap;
	text-overflow: ellipsis;
	position: absolute;
	top: 2vw;
	right: 1vw;
	background: #eee;
	border-top-left-radius: 4vw;
	border-bottom-left-radius: 4vw;
}

.section_near_product .more {
	width: 100%;
	height: 11vw;
	color: #646464;
	line-height: 10vw;
	text-align: center;
	font-size: 4vw;
	font-weight: 300;
}

.good_comment {
	width: 100%;
}

.good_comment span.star_span {
	width: 4vw;
	height: 5vw;
	float: left;
	background-position: 0 center;
	background-size: 3vw 3vw;
	background-repeat: no-repeat
}

.good_comment span.star_pf {
	color: red;
	font-size: 2.5vw;
	float: left;
	line-height: 5vw
}

.good_comment span.xl {
	color: #333;
	font-size: 2.5vw;
	float: right;
	line-height: 5vw
}

.swiper-pagination-bullet-active {
	background-color: #333
}

.zero_star_icon {
	background-image: url(data:image/png;base64,)
}

.self_star_icon {
	background-image: url(data:image/png;base64,)
}

.one_star_icon {
	background-image: url(data:image/png;base64,)
}

.address_black_icon {
	background-image: url(data:image/png;base64,)
}

.search_white_icon {
	background-image: url(data:image/png;base64,)
}

.address_white_icon {
	background-image: url(data:image/png;base64,)
}


/***********************************************************************搜索******************************************************************************/

.dis_ea {
	display: flex;
}

.dis_ea input[type=text] {
	border: 0;
	width: 77%;
	padding: 1vw 4vw;
	height: 8vw;
	margin-top: 2vw;
	margin-left: 3vw;
	margin-bottom: 0;
	font-size: 4vw;
}

.dis_ea input[type=text]::-webkit-input-placeholder {
	color: #ACACAC;
}

.dis_ea input[type=text]::-moz-placeholder {
	color: #ACACAC;
}


/* Mozilla Firefox 19+ */

.dis_ea input[type=text]:-moz-placeholder {
	color: #ACACAC;
}


/* Mozilla Firefox 4 to 18 */

.dis_ea input[type=text]:-ms-input-placeholder {
	color: #ACACAC;
}


/* Internet Explorer 10-11 */

.dis_ea p {
	margin-bottom: 0;
	line-height: 12vw;
	color: #fff;
	margin-left: 1vw;
}

.sech_his {
	padding: 3vw 2.5vw;
	display: flex;
	justify-content: space-between;
	align-items: center;
	font-size: 4vw;
}

.sech_his p {
	color: #000;
	margin-bottom: 0;
}

.sech_list {
	padding: 2.5vw;
}

.sech_list ul {
	width: 100%;
	padding: 0;
	margin: 0;
}

.sech_list ul li {
	float: left;
	padding: 1vw 2vw;
	background: #f2f2f2;
	margin-right: 3vw;
	margin-bottom: 3vw;
}

