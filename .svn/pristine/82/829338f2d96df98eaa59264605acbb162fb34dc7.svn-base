/*弹窗*/
.modal {
	display: none;
	position: fixed;
	z-index: 999;
	padding-top: 100px;
	left: 0;
	top: 0;
	width: 100%;
	height: 100%;
	overflow: auto;
	background-color: rgb(0, 0, 0);
	background-color: rgba(0, 0, 0, 0.5);
}

/* 弹窗内容 */
.modal-content {
	border-top-left-radius: 2vw;
	border-top-right-radius: 2vw;
	background: #f5f5f5;
	position: fixed;
	bottom: 0;
	left: 0;
	padding: 0;
	width: 100%;
	-webkit-animation-name: animatetop;
	-webkit-animation-duration: 0.4s;
	animation-name: animatetop;
	animation-duration: 0.4s
}

.modal-content .content_h6 {
	border-radius: 2vw 2vw 0 0;
	width: 100vw;
	height: 9.26vw;
	background: #fff;
	color: #333;
	font-size: 3.9vw;
	text-align: center;
	line-height: 9.26vw;
	position: relative;
}

.modal-content .content_h6 .cont_img {
	position: absolute;
	right: 4vw;
	top: 1.3vw;
}

/* 添加动画 */
@-webkit-keyframes animatetop {
	from {
		bottom: -300px;
		opacity: 0
	}
	to {
		bottom: 0;
		opacity: 1
	}
}

@keyframes animatetop {
	from {
		bottom: -300px;
		opacity: 0
	}
	to {
		bottom: 0;
		opacity: 1
	}
}

