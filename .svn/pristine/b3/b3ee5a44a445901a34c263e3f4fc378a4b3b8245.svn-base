/*提交内容*/
/*改变input字体颜色*/

input::-webkit-input-placeholder {
	color: #ccc;
}

input::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: #ccc;
}

input:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: #ccc;
}

input:-ms-input-placeholder {
	/* Internet Explorer 10-11 */
	color: #ccc;
}
.submit_container{
	width: 421px;
	margin: 0 auto;
	margin-top: 32px;
}
 
.submit_list{
	margin-bottom: 29px; 
	overflow: hidden;
}
.submit_list span{
	border: 1px solid #ddd;
	display: inline-block;
	width: 106px;
	height: 45px;
	line-height: 45px;
	padding-left: 20px;
	color: #333;
	font-size: 14px;
	float: left;
	border-right:0 ;
}
.submit_list .ph{
	border-right: 1px solid #ddd;
}
.submit_list input{
	border: 1px solid #ddd;
	width: 315px;
	border-left: 0;
	height: 45px;
	line-height: 45px;
	padding-left: 30px;
	font-size: 13px;
	float: right;
}

