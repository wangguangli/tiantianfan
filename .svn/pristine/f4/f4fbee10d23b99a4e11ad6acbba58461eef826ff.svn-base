	*{margin: 0;padding: 0;list-style: none;text-decoration: none;}
	body{}
	a,a:hover{color: #999;text-decoration: none;}	
	.product-leibie{width: 30%;left: 0;top: 0;position: fixed;height: 100%;background: #fafafa;}	
	.product-leibie ul {width: 100%;padding-top: 12vw;}
	.product-leibie ul li{text-align: center;line-height: 14vw;font-size: 14px;position: relative;}
	.product-leibie ul li.bc-gray span{width: 1.5vw;height: 7vw;background: #000;position: absolute;top: 3.5vw;left: 0;}
	.product-erji{margin-left: 30%;width: 70%;padding-top: 16vw;}	
