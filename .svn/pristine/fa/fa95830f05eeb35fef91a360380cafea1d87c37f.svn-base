							html += "<span onclick='deleteorder()'>删除订单</span>";
							html += "</div></div>";
						})
						$('#cancel').html(html);
					}
				}
			})
		}
function deleteorder() {
	var orders_id = $("#orders_id").val();
	$.ajax({
		type : "POST",
		dataType: "json",
		url : "{:U('Api/Orderin/deleteOrder')}",
		data : {order_id:orders_id},
		success:function (data) {
			layer.open({
				content: "删除成功"
				,skin: 'msg'
				,time: 2
			});
		}
	});

}
