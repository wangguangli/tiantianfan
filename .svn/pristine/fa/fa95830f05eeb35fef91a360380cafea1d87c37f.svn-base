			//console.log(str)
			var result={};
			//分割字符串  -->产生字符串数组
			var strs=str.split("&");
			//console.log(strs)
			//遍历数组中的每一个元素
			strs.forEach(function(v){
			 //console.log(v)
				//伪代码：v="age=18"
				var keyvalue=v.split("=");
			//console.log(keyvalue)
				var name=keyvalue[0];
		
				var value=keyvalue[1];
						console.log(name)
				result[name]=value;
			})
			return result;
		}
