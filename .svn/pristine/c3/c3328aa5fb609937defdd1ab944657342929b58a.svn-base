										<input type="checkbox" name="" value="" title="下三级金额" lay-skin="primary" <if condition="$data['up_auto_third_money']!=0.00 && $data['up_auto_third_money']!=null ">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_third_money" value="{$data['up_auto_third_money']}">
										<span class="input-group-addon">元</span>

										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_third_money_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_third_money_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="本团队金额" lay-skin="primary" <if condition="$data['up_auto_team_money']!=0.00 && $data['up_auto_team_money']!=null">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_team_money" value="{$data['up_auto_team_money']}">
										<span class="input-group-addon" >元</span>
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_team_money_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_team_money_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="下一级订单" lay-skin="primary"  <if condition="$data['up_auto_first_order']">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_first_order" value="{$data['up_auto_first_order']}">
										<span class="input-group-addon" >个</span>
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_first_order_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_first_order_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
