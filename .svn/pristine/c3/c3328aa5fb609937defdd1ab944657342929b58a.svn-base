<script>


	layui.use('form',function(){
		var  form = layui.form;
		var  table = layui.table;
		var  $ = layui.jquery;
		form.render();

		form.on('select', function(data){
			$(data.elem).trigger('change', data.elem)
		});


		form.on('switch(test)',function(data) {

			var display = $('.xian').css('display');
			if(display == 'none'){
				$('.xian').css('display','block');
			}else{
				$('.xian').css('display','none');
			}
		})

		form.on('switch(zfsj)',function(data){
			var display = $('#zfsj').css('display');
			if(display == 'none'){

				$('#zfsj').css('display','block');
				$('#up_level_money').attr('required','true');
			}else{
				$('#up_level_money').removeAttr('required');
				$('#zfsj').css('display','none');

			}

		})

		form.on('switch(zdsj)',function(data){
			var display = $('#zdsj').css('display');
			if(display == 'none'){
				$('#zdsj').css('display','block');
				$('#up_auto_mode').attr('checked','checked');

			}else{
				$('#zdsj').css('display','none');

			}
		})

		form.on('switch(zdjj)',function(data){
			var display = $('#zdjj').css('display');
			if(display == 'none'){
				$('#zdjj').css('display','block');
			}else{
				$('#zdjj').css('display','none');
			}
		})

		form.on('switch(tjjl)',function(data){
			var display = $('#tjjl').css('display');
			if(display == 'none'){
				$('#tjjl').css('display','block');
			}else{
				$('#tjjl').css('display','none');
			}
		})

		form.on('switch(xffy)',function(data){
			var display = $('#xffy').css('display');
			if(display == 'none'){
				$('#xffy').css('display','block');
			}else{
				$('#xffy').css('display','none');
			}
		})

		form.on('switch(fgfy)',function(data){
			var display = $('#fgfy').css('display');
			if(display == 'none'){
				$('#fgfy').css('display','block');
			}else{
				$('#fgfy').css('display','none');
			}
		})
	});

	layui.use('upload', function(){
		var upload = layui.upload;

		//执行实例
		var uploadInst = upload.render({
			elem: '#upimg' 							//绑定元素
			,url: '/index.php/Admin/Common/uploads' 	//上传接口
			,data:{handlename:"headimg"}			// 接口接收值
			,field:"headimg"						// input name，与接口接收值 一致
			,done: function(res)
			{
				//上传完毕回调-不一定成功，需要判断
				console.log(res);
				if (res.status > 0)
				{
					// 异常提示
					layer.alert(res.result);
				}
				else
				{
					$(".headimgurl").val(res.result);
					$(".img_headimgurl").attr("src", res.result);
				}
			}
			,error: function(e)
			{
				//请求异常回调
				console.log(e);
			}
		});
	});

	function sub(){
		var status = $('input[name="status"]:checked').val();

		if(status == null){
			alert('等级状态必须选择!');
			return false;
		}

		var up_level_by_auto = $('input[name="up_level_by_auto"]:checked').val();
		if(up_level_by_auto == 'on'){
			var up_auto_mode =  $('input[name="up_auto_mode"]:checked').val();

			if(up_auto_mode == null){
				alert('升级条件必须选择!');
				return false;
			}
		}
		var down_level_by_auto = $('input[name="down_level_by_auto"]:checked').val();
		if(down_level_by_auto == 'on'){
			var down_auto_mode =  $('input[name="down_auto_mode"]:checked').val();

			if(down_auto_mode == null){
				alert('降级条件必须选择!');
				return false;
			}
		}

		var recommend_status = $('input[name="recommend_status"]:checked').val();
		if(recommend_status == 'on'){
			var recommend_level_diff =  $('input[name="recommend_level_diff"]:checked').val();

			if(recommend_level_diff == null){
				alert('是否级差必须选择!');
				return false;
			}

			var recommend_mode =  $('input[name="recommend_mode"]:checked').val();

			if(recommend_mode == null){
				alert('推荐奖励类型必须选择!');
				return false;
			}
		}

		var buy_award_status = $('input[name="buy_award_status"]:checked').val();
		if(buy_award_status == 'on'){
			var buy_award_mode =  $('input[name="buy_award_mode"]:checked').val();

			if(buy_award_mode == null){
				alert('首购返佣奖励类型必须选择!');
				return false;
			}


		}
		var repeat_buy_award_status = $('input[name="repeat_buy_award_status"]:checked').val();
		if(repeat_buy_award_status == 'on'){

			var repeat_buy_award_mode =  $('input[name="repeat_buy_award_mode"]:checked').val();

			if(repeat_buy_award_mode == null){
				alert('复购返佣奖励类型必须选择!');
				return false;
			}
		}
		return true;
	}
	$(function(){
		$(".bj_x").click(function() {
			if (!($(this).children('input[type="checkbox"]').prop("checked"))) {
				$(this).next().find('input[type="text"]').val('0');
				$(this).next().find('.layui-unselect').val('请选择');
                $(this).next().find("option:selected").attr("selected", false);
                $(this).next().find(".layui-anim-upbit dd").removeClass(".layui-this");
			}
		});
	})

	// 升级 分类选择
	function changeCate(id, txt) {
		var cate_div = $("#cate_div").clone(true).text(txt).removeAttr("id").attr("cid", "c" + id);
		var cate_ipt = $("#cate_ipt").clone(true).val(id).removeAttr("id").addClass("c" + id);
		$(".cate_list").append(cate_div);
		$(".cate_list").append(cate_ipt);
	}
	$(".cate_list").on("click", ".ext_cate", function () {
		var that = $(this);
		var cid = that.attr("cid");
		if (cid == "c999999999") {
			return true;
		}
		layer.confirm('是否删除此附属分类?', {icon: 3, title: '提示'}, function (index) {

			console.log(that);
			console.log(cid);
			if (cid) {
				$(".cate_list").find("." + cid).remove();
				that.remove();
			} else {
				layer.msg('删除失败，请尝试刷新');
			}

			layer.close(index);
		});
	});

