										<input type="checkbox" name=""  value="" title="下一级等级" lay-skin="primary" <if condition="$data['up_auto_first_leader_num']">checked</if>>
									</div>
									<div class="input-group bj_nr" >
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_first_leader_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_first_leader_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
										<input type="text" class="form-control" placeholder="" name="up_auto_first_leader_num" value="{$data['up_auto_first_leader_num']}">
										<span class="input-group-addon" >人</span>


									</div>
								</div>

								<div  class="bj">
									<div  class="bj_x">
										<input type="checkbox" name="" value="" title="下二级等级" lay-skin="primary"  <if condition="$data['up_auto_second_leader_num']">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_second_leader_level" lay-verify="" >
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_second_leader_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
										<input type="text" class="form-control" placeholder="" name="up_auto_second_leader_num" value="{$data['up_auto_second_leader_num']}">
										<span class="input-group-addon" >人</span>

									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name=""  value="" title="下三级等级" lay-skin="primary" <if condition="$data['up_auto_third_leader_num']">checked</if>>
									</div>
									<div class="input-group bj_nr">
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_third_leader_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_third_leader_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
										<input type="text" class="form-control" placeholder=""  name="up_auto_third_leader_num" value="{$data['up_auto_third_leader_num']}">
										<span class="input-group-addon" >人</span>
									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="本团队等级" lay-skin="primary"  <if condition="$data['up_auto_team_num']">checked</if>>
									</div>
									<div class="input-group bj_nr">
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_team_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_team_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
										<input type="text" class="form-control" placeholder="" name="up_auto_team_num" value="{$data['up_auto_team_num']}">
										<span class="input-group-addon" >人</span>
									</div>
								</div>



								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="本人消费金额 " lay-skin="primary" <if condition="$data['up_auto_self_money']!=0.00 && $data['up_auto_self_money']!=null">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_self_money" value="{$data.up_auto_self_money}">
										<span class="input-group-addon" >元</span>
									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="下一级金额" lay-skin="primary"  <if condition="$data['up_auto_first_money']!=0.00 && $data['up_auto_first_money']!=null ">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_first_money" value="{$data['up_auto_first_money']}">
										<span class="input-group-addon" >元</span>

										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_first_money_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_first_money_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
										</div>
										<span class="input-group-addon" >等级</span>
									</div>
								</div>

								<div class="bj">
									<div class="bj_x">
										<input type="checkbox" name="" value="" title="下二级金额" lay-skin="primary" <if condition="$data['up_auto_second_money']!=0.00 && $data['up_auto_second_money']!=null ">checked</if>>
									</div>
									<div class="input-group bj_nr"  >
										<input type="text" class="form-control" placeholder="" name="up_auto_second_money" value="{$data['up_auto_second_money']}">
										<span class="input-group-addon" >元</span>
										<div class="form-control" style="margin: 0;padding: 0;">
											<select name="up_auto_second_money_level" lay-verify="">
												<option value="0">请选择</option>
												<volist name="level" id="vo">
													<option value="{$vo.level_ranking}" <if condition="$vo['level_ranking']==$data['up_auto_second_money_level']">selected</if>>{$vo.level_name}</option>
												</volist>
											</select>
