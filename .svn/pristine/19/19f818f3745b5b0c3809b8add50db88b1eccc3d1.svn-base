/*头部*/
/*商城首页*/

.grid_c1 {
	width: 1200px;
	margin: 0 auto;
	overflow: hidden;
}
/*header*/

.head_img {
	width: 100%;
	height: auto;
}

.head_img img {
	width: 100%;
	height: 100%;
	vertical-align: middle;
}
/*头部登录*/

.hed_login {
	width: 100%;
	height: 28px;
	line-height: 24px;
	background-color: #e3e4e5;
}
.hed_login-cont em {
	color: #333;
	margin: 0 5px;
}
.hed_login-cont .ty{
	width: 10px;
	height: 10px;
	margin-left: 5px;
}

.hed_login a {
	color: #333;
	font-size: 12px;
}

.hed_login a:hover {
	color: #df1a0f;
}

.hed_login-cont {
	text-align: right;
	padding-right: 173px;
}
/*logo*/
.grid_c1 .logo{
	float: left;
	margin-right: 64px;
	margin-top: 13px;
}
.grid_c1 .logo a{
	display: block;
	width: 100%;
}
.grid_c1 .logo a img{
	width: 163px;
	height: auto;
}
 
/*头部的搜索框部分*/

.header02 {
	width: 100%;
	height: auto;
	overflow: hidden;
	background-color: #fff;
	padding-bottom: 15px;
}

.header_left {
	width:67%;
	float: left;
	overflow: hidden;
	margin-top: 43px;
}

.search_box {
	/*width: 709px;*/
	height: 36px;
	float: left;
}

.search_box .text {
	width: 600px;
	font-size: 12px;
	height: 34px;
	border: 2px solid #e2231a;
	padding-left: 17px;
	float: left;
}
input::-webkit-input-placeholder {
    color: #999;
  }
 
  input::-moz-placeholder {
    /* Mozilla Firefox 19+ */
    color: #999;
  }
 
  input:-moz-placeholder {
    /* Mozilla Firefox 4 to 18 */
    color: #999;
  }
 
  input:-ms-input-placeholder {
    /* Internet Explorer 10-11 */
    color: #999;
  }
.search_box .button {
	width: 58px;
	border: none;
	background-color: #e1251b;
	font-size: 20px;
	height: 40px;
	 
	 
}

.search_box .button img {
	width: 20px;
	height: 20px;
	margin-top: 5px;
}
 

.shopping_cart {
	width: 17px;
	height: 18px;
	margin-left: 10px;
	float: left;
	margin-top: 6px;
}

.shopping_cart img {
	width: 100%;
	height: 100%;
}

.shopping-icon .round {
	width: 16px;
	height: 16px;
	border-radius: 50%;
	float: left;
	background-color: #df1a0f;
	text-align: center;
	color: #fff;
	font-size: 12px;
}
.shopping-text {
	font-size: 14px;
	color: #df1a0f;
	margin-left: 7px;
	margin-top: 5px;
	display: inline-block;
}
.Small_navigation {
	width: 500px;
    padding-left: 71px;
	overflow: hidden;
	margin-top: 43px;
}

.Small_navigation a {
	font-size: 12px;
	color: #999;
	margin-right: 8px;
}
.Small_navigation a:hover {
	color: #df1a0f;
}
/*首页导航*/
.page_navigation {
	width: 100%;
	overflow: hidden;
	font-size: 15px;
	margin-top: 12px;
	padding-left: 71px;
	color: #333;
}
.page_navigation_left {
	width: 119px;
	float: left;
}
.page_navigation_left img {
	width: 15px;
	height: 15px;
	float: left;
	margin-top: 3px;
	margin-right: 10px;
}
.page_navigation_left a {
	color: #df1a0f;
}
.page_navigation em {
	color: #df1a0f;
	margin: 0 10px;
}
.page_navigation .nav {
	float: left;
	width: 69%;
}
.page_navigation .nav a {
	margin: 0 19px;
	color: #333;
}
.page_navigation a:hover {
	color: #df1a0f;
}
.personal_center a {
	color: #df1a0f;
}
/*右边二维码*/
.header_right{
	width: 65px;
	height: 65px;
	margin-top:58px;
	float: right;
}
.header_right img{
	width:100%;
	height: 100%;
}


/*公用底部*/
.footer {
	width: 100%;
	overflow: hidden;
}
.mod_service {
	padding: 30px 0;
	border-bottom: 1px solid #dedede
}
.mod_service_list {
	height: 42px
}
.mod_service_item {
	float: left;
	width: 321px
}
.mod_service_item02 {
	width: 235px
}
.mod_service_txt {
	width: 176px;
	float: left;
	margin-left: 10px;
	height: 42px;
	line-height: 42px;
	font-size: 18px;
	font-weight: 700;
	text-overflow: ellipsis;
	white-space: nowrap;
	color: #444
}
.mod_help {
	padding: 30px 0 20px 0;
}
.mod_help_list {
	overflow: hidden;
	height: 160px
}
.mod_help_nav {
	float: left;
	width: 75px;
	text-align: center;
	line-height: 22px;
	margin: 0 62px;
}
.mod_help_nav_tit {
	font-size: 16px;
	font-weight: bold;
	margin-bottom: 15px;
}
.mod_copyright_inner {
	padding: 15px 0;
	border-top: 1px solid #e1e1e1;
	text-align: center
}
.mod_copyright_info {
	padding: 10px 0;
	line-height: 22px;
	color: #999
}
.mod_copyright_info a {
	color: #999
}
.mod_copyright_info a:hover {
	color: #c81623
}
/*底部有边框的字*/
.mod_service_tit {
	width: 39px;
	height: 39px;
	line-height: 34px;
	border-radius: 50%;
	border: 2px solid #e23441;
	color: #e23441;
	font-size: 19px;
	font-weight: bold;
	text-align: center;
	float: left;
}
/*底部链接*/
.mod_help_nav_con a {
	font-size: 14px;
	color: #444;
	line-height: 28px;
}
.mod_help_nav_con a:hover {
	color: #df1824;
}

 
/*城市选择框*/
.City_selection{
	width: 70px;
	overflow: hidden;
	float: left;
	margin-top: 7px;
}
.City_selection img{
	float: left;
	width: 11px;
	height: 17px;
	margin-right: 2px;
	margin-top: 3px;
}
.City_selection .cont_list{
	width: 57px;
	float: left;
}
.cont_list select{
	border: 1px solid #fff;
	font-size: 13px;
	color: #333;
}
 
.head_portrait .cont_list input{
	 border: 1px solid #eee;
	 width: 292px;
	 height: 30px;
	 margin-top: 10px;
}
.head_portrait .cont_list .input_yuan{
	width: 130px;
	margin-left: 3px;
}
.cont_list .yuan{
	color: #999 !important;
	margin-left: 3px;
}




