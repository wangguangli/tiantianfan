/* CSS Document */


/*商家中心*/

body {
	background-color: #f5f6fa
}

.wappalls {
	width: 100%;
	padding: 0 3.5vw;
	background: url(../images/shopcenter1.png) top no-repeat #f5f6fa;
	background-size: 100% auto;
}

.sjtop {
	display: flex;
	padding-top: 18vw;
	align-items: center;
}

.sjtop img {
	width: 20vw;
	height: 20vw;
	border-radius: 25vw;
