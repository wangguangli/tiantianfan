	width: 95%;
	height: 1px;
	background-color: #ddd;
	border: 0;
	display: block;
	margin-top: 4vw;
}

.detail {
	width: 95%;
	margin: 4vw auto 0;
}

.detail a {
	display: flex;
	align-items: center;
}

.detamg1 {
	width: 6vw;
	height: 6vw;
}

.detail h4 {
	font-size: 4vw;
	color: #999;
	margin: 0 0 0 2vw;
	font-weight: 500;
}

.detamg2 {
	width: 2.6vw;
	margin-left: auto;
}

.zclist {
	width: 100%;
	margin: 1.5vw auto 0;
	box-sizing: border-box;
	display: table;
}

.zclist ul {
	margin: 0 -1vw;
	padding: 0;
}

.zclist ul li {
	width: 1%;
	display: table-cell;
	padding: 0 1vw
}

.zclist ul li a {
	background-color: #fff;
	border-radius: 1vw;
	text-align: center;
	display: table;
	width: 100%;
	padding: 3vw 2vw;
}

.zclist ul li a p {
	font-size: 3.6vw;
	color: #999;
	margin: 0;
}

.zclist ul li a h2 {
	font-size: 4.5vw;
	color: #000;
	font-weight: 500;
	padding-top: 2vw;
}

.guanlis {
	width: 100%;
	background-color: #fff;
	border-radius: 1vw;
	margin: 1.5vw auto 3vw;
	padding: 3vw;
}

.guanlis h2 {
	font-size: 4.5vw;
	color: #000;
	font-weight: 500;
}

.guanlis hr {
	width: 100%;
	height: 1px;
	background-color: #ddd;
	border: none;
	display: block;
	margin: 4vw 0 0 0;
}

.guanlis ul {
	margin: 0;
	padding: 0;
}

.guanlis ul li {
	width: 100%;
	padding: 5vw 0;
	border-bottom: 1px solid #ddd;
}

.guanlis ul li:last-child {
	border: none;
}

.guanlis ul li a {
	display: flex;
	align-items: center;
}

.gulimg1 {
	width: 6vw;
	height: 6vw;
}

.guanlis ul li a p {
	font-size: 4vw;
	color: #555;
	display: block;
	margin: 0 0 0 4vw;
}

.gulimg2 {
	width: 2.6vw;
	margin-left: auto;
}


