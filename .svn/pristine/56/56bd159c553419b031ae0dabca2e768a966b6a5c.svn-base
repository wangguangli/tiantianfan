	background: url(../images/shopcenter2.png) center no-repeat;
	background-size: 100% auto;
	box-sizing: border-box;
}

.jit-right a {
	display: flex;
	align-items: center;
	justify-content: center;
	text-align: center;
	line-height: 18vw;
}

.jit-right img {
	width: 6vw;
	height: 4.5vw;
}

