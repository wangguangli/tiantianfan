<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!--<meta name="viewport" content="width=device-width, initial-scale=1">-->
    <title>{:GC('web_title')}管理后台-打印设置</title>
    <include file="Public/asset_top_page"/>
    <style>
        tr{
            border-bottom: 1px solid #ddd;
        }
    </style>
</head>
<body>
<!-- header -->
<include file="Public/header"/>
<!-- /header -->
<!-- Page container -->
<div class="page-container">
    <!-- Page content -->
    <div class="page-content">
        <!-- main sidebar -->
        <include file="Public/left"/>
        <!-- /main sidebar -->

        <!-- Main content -->
        <div class="content-wrapper">

            <!-- Content area -->
            <div class="content" style="padding-top: 10px;">
                <!-- Pagination types -->
                <div class="v-container">
                    <div class="alert alert-tips alert-dismissible" role="alert">
                        <button type="button" class="close" data-dismiss="alert" aria-label="Close"><span aria-hidden="true">×</span></button>
                        本页打印设置</div>

                    <div class="panel panel-flat">
                        <div class="panel-heading">
                            <form class="v-filter-container" action="" method="get">

                                <div class="filter-fields-wrap">
                                    <div class="filter-item clearfix">
                                        <div class="filter-item__field">



                                        </div>
                                    </div>


                                    <div class="filter-item clearfix">
                                        <div class="filter-item__field">
                                            <div class="v__control-group padd_left_11">

                                                <div class="v__controls">
                                                    <!--													<a class="btn btn-primary search">搜索</a>-->
                                                    <a href="{:U('Index/stamp_detail')}"><input type="button" value="添加" class="btn btn-primary search" style="width:100px;height: 35px;"></a>
                                                    <a class="btn btn-success ml-15 dataExcel" style="display: none;">导出订单</a>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </form>

                            <!--搜索框-->
                            <div class="heading-elements">

                            </div>
                        </div>


                        <div class="dataTables_wrapper no-footer">
                            <table class="table datatable-tools-basic basic_table">
                                <thead>
                                <tr>
                                    <th style="width: 1px;"></th>
                                    <th>快递打印名称</th>
                                    <th>状态</th>
                                    <th class="text-center">操作</th>

                                </tr>
                                </thead>

                                <volist name="list" id="vo">
                                    <tr class="tt_color">
                                        <td style="width: 1px;">
                                        </td>


                                        <td class="">
                                            <div class="">
                                                <a href="{:U('index/stamp_detail')}?id={$vo.id}" style="color: #0b76cc">{$vo.name}</a>

                                            </div>
                                        </td>



                                        <td class="">
                                            <div class="ed_stand">
                                                <p>
                                                    <if condition="$vo['is_start'] == 1">
                                                        <a href="{:U('index/stamp_upd')}?id={$vo.id}&is_start={$vo.is_start}"><font
                                                                style="color:red">[关闭]</font></a>
                                                        <else/>
                                                        <a href="{:U('index/stamp_upd')}?id={$vo.id}&is_start={$vo.is_start}"><font
                                                                style="color:green">[开启]</font></a>
                                                    </if>
                                                </p>
                                            </div>
                                        </td>
                                        <td class="text-center">
                                            <div class="tt_stand">
                                                <a title="查看详情" href="{:U('Index/stamp_detail')}?id={$vo.id}" class="layui-btn layui-btn-sm layui-btn-normal btn_detail">详情</a>
                                                <a title="删除" href="{:U('Index/stamp_delete', 'id=' . $vo['id'])}" class="layui-btn layui-btn-sm layui-btn-danger btn_detail" onclick="return confirm('确定要删除吗?');">删除</a>
                                            </div>
                                        </td>
                                    </tr>
                                </volist>
                            </table>



                            <!-- 分页 -->
                        </div>
                        <!-- /pagination types -->

                    </div>
                    <!-- Footer -->
                    <include file="Public/footer" />
                    <!-- /footer -->

                </div>
                <!-- /content area -->

            </div>
            <!-- /main content -->


        </div>
    </div>

        <!-- /page content -->

    </div>
    <!-- /page container -->

</body>
</html></html>