/*登录页面style*/
.loginbg{background:#000 no-repeat; background-size:100% 100%; position:absolute; width:100%; height:100%}
.title-tio{width:100%; display:inline-block; margin-bottom:10px; text-align:center; background:url(../images/title-tio.png) no-repeat center; color: #fff; font-size:18px;}
.login-container .dlbg{
    width: 100%;
    height: 300px;
    background-color: #fff;
    border: none;
    border-radius: 5px;
}
.login-container .dlbg .dlbg_login{color: #000;font-weight: 400;font-size: 20px;text-align: center;padding: 30px 0;}
.loginbox-textbox{position: relative;width: 307px;margin: 0 auto 11px auto; border: 1px solid #cccccc;}
.loginbox-textbox .input_name{
    width: 270px;
    height: 32px;
    line-height: 24px;
    padding-left: 5px;  
    font-size: 14px;
    font-family: arial,"宋体";outline: none;border: 0;background: #fff;}

	input:-webkit-autofill,input:-webkit-autofill:hover,input:-webkit-autofill:focus {
				box-shadow:0 0 0 60px #fff inset;
				-webkit-text-fill-color: #000;
	}
	.loginbox-submit{width: 307px;margin: 0 auto;}
	.account{width: 307px;display: flex;justify-content: space-between;margin:0 auto;margin-top: 15px;color: #999;font-size: 13px;}
.file-up {
    position: relative;
    width: 94px;
    height: 25px;
	display:table;
	margin-top:5px;
	}
.file-up input[type='file'] {
    width: 120px;
    height: 25px;
    appearance: none;
    -webkit-appearance: none;
    opacity: 0;
    position: relative;
    z-index: 100;
	}
.file-up .file-bg {
    width: 100%;
    height: 100%;
    position: absolute;
    background:#e7e7e7;
	border:1px solid #939393;
	line-height:25px;
	text-align:center;
    top: 0;
    left: 0;
    z-index: 1;
    display: table;
	border-radius:3px;
}
.loginbg .login-bg-img img {
    display: block;
    width: 100%;
    -webkit-animation: turnZ 100s linear infinite;
    animation: turnZ 100s linear infinite;
}
.loginbg img {
    vertical-align: middle;
    list-style: none;
}
.loginbg .login-bg-img {
    position: fixed;
    left: 20%;
    bottom: -50%;
    width: 60%;
    z-index: 0;
}
@-webkit-keyframes turnZ {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg)
	}
	to {
		-webkit-transform: rotate(1turn);
		transform: rotate(1turn)
	}
}		
@keyframes turnZ {
	0% {
		-webkit-transform: rotate(0deg);
		transform: rotate(0deg)
	}
	to {
		-webkit-transform: rotate(1turn);
		transform: rotate(1turn)
	}
}

